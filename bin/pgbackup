#!/usr/bin/env bash

__SRC__="$(dirname "$(dirname "${BASH_SOURCE[0]}")")/src"

###>include
source "${__SRC__}/pg.bash" || {
    echo "[!] Failed to source pg.bash." >&2


    exit 1
}

source "${__SRC__}/pg/con.bash" || {
    echo "[!] Failed to source pg/con.bash." >&2

    exit 1
}
###<include

pg_backup "$@"
