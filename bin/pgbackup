#!/usr/bin/env bash

###>embedded

__SRC__="$(dirname "$(dirname "${BASH_SOURCE[0]}")")/src"

source "${__SRC__}/pg.bash" || {
    echo "[!] Failed to source pg.bash" >&2

    exit 1
}

source "${__SRC__}/pg/backup.bash" || {
    echo "[!] Failed to source pg/backup.bash" >&2

    exit 1
}

###<embedded


pg_backup "$@"
