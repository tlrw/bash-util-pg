#!/usr/bin/env bash

if [[ "${BASH_VERSINFO[0]:-0}" -lt 4 ]]; then
    echo "pgbackup: requires Bash in version 4 or higher, current version: ${BASH_VERSION}"

    exit 1
fi


__DIR__="$(dirname -- "$(dirname -- "${BASH_SOURCE[0]}")")"

source "${__DIR__}/lib/tlrw/bash/src/std.bash" "util" || {
    exit 1
}

inc_module "${__DIR__}/src/pgcon"
